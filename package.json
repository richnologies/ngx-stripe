{
  "name": "ngx-stripe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:lib": "ng build ngx-stripe",
    "build:app": "ng build ngx-stripe --prod",
    "pack:lib": "cd dist/ngx-stripe && npm pack",
    "copy:readme": "cpx README.md dist/ngx-stripe",
    "copy:license": "cpx LICENSE.md dist/ngx-stripe",
    "package": "npm run build:lib && npm run copy:readme && npm run copy:license && npm run pack:lib",
    "publish": "npm publish dist/ngx-stripe/ngx-stripe-X.X.X.tgz",
    "start": "ng serve ngx-stripe-tester",
    "test": "ng test ngx-stripe",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:modern": "ng build --plugin ngx-build-modern --prod",
    "build:modern:ngx-stripe-tester": "ng build --plugin ngx-build-modern --project ngx-stripe-tester --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.13.0",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-build-modern": "^1.1.7",
    "ngx-build-plus": "^7.7.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
